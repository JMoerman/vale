procedure defensivejump(operand targetaddr:int, ghost fname:string, ghost env:umemlayout)
        requires
            address_within_procedure(fname, targetaddr, env);
        modifies
            rip;
        ensures
            rip == targetaddr;
{
            JumpShort(targetaddr);
}
